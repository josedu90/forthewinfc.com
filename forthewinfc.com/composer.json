  "require": {
    "php": ">=7.1",
    "composer/installers": "^1.7",
    "vlucas/phpdotenv": "^3.4.0",
    "oscarotero/env": "^1.2.0",
    "roots/wordpress": "5.2.3",
    "roots/wp-config": "1.0.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "josedu90/champion": "*",
    "josedu90/champion-child": "*",
    "josedu90/woocommerce": "*",
    "josedu90/wordpress-importer": "*",
    "josedu90/wp-mailgun-smtp": "*",
    "josedu90/ivan-visual-composer": "*",
    "josedu90/mega_main_menu": "*",
    "josedu90/sportspress-pro": "*",
    "josedu90/suspended-lists-for-sportspress": "*",
    "josedu90/tinymce-advanced": "*",
    "josedu90/stm-gdpr-compliance": "*",
    "josedu90/ts-visual-composer-extend": "*",
    "josedu90/revslider": "*",
    "josedu90/sportspress-for-soccer": "*",
    "josedu90/material-admin": "*",
    
    "josedu90/js_composer": "*"
    

  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.4.2",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test": [
      "phpcs"
    ]
  }
}
